<?xml version="1.0" ?>
<sdf version="1.8">
  <model name='four_wheel_robot'>
    <pose>0 0 0.3 0 0 0</pose>
    
    <!-- Chassis (car body) -->
    <link name='chassis'>
      <pose>0 0 0.20 0 0 0</pose>
      <inertial>
        <mass>10.0</mass>
        <inertia>
          <ixx>1.0</ixx> <iyy>1.5</iyy> <izz>1.0</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <box><size>1.0 0.6 0.3</size></box>
        </geometry>
        <material>
          <ambient>0 0 1 1</ambient>
          <diffuse>0 0 1 1</diffuse>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <box><size>1.0 0.6 0.3</size></box>
        </geometry>
      </collision>
    </link>
    
    <!-- Front Left Wheel -->
    <link name='front_left_wheel'>
      <pose>0.4 0.35 0.15 -1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.1</ixx> <iyy>0.1</iyy> <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1</ambient>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode><mu>1.0</mu><mu2>1.0</mu2></ode>
          </friction>
        </surface>
      </collision>
    </link>
    
    <!-- Front Right Wheel -->
    <link name='front_right_wheel'>
      <pose>0.4 -0.35 0.15 -1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.1</ixx> <iyy>0.1</iyy> <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1</ambient>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode><mu>1.0</mu><mu2>1.0</mu2></ode>
          </friction>
        </surface>
      </collision>
    </link>
    
    <!-- Rear Left Wheel -->
    <link name='rear_left_wheel'>
      <pose>-0.4 0.35 0.15 -1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.1</ixx> <iyy>0.1</iyy> <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1</ambient>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode><mu>1.0</mu><mu2>1.0</mu2></ode>
          </friction>
        </surface>
      </collision>
    </link>
    
    <!-- Rear Right Wheel -->
    <link name='rear_right_wheel'>
      <pose>-0.4 -0.35 0.15 -1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.1</ixx> <iyy>0.1</iyy> <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual name='visual'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1</ambient>
        </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode><mu>1.0</mu><mu2>1.0</mu2></ode>
          </friction>
        </surface>
      </collision>
    </link>
    
    <!-- Joints - Front Left -->
    <joint name='front_left_wheel_joint' type='revolute'>
      <parent>chassis</parent>
      <child>front_left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.5</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>
    
    <!-- Joints - Front Right -->
    <joint name='front_right_wheel_joint' type='revolute'>
      <parent>chassis</parent>
      <child>front_right_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.5</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>
    
    <!-- Joints - Rear Left -->
    <joint name='rear_left_wheel_joint' type='revolute'>
      <parent>chassis</parent>
      <child>rear_left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.5</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>
    
    <!-- Joints - Rear Right -->
    <joint name='rear_right_wheel_joint' type='revolute'>
      <parent>chassis</parent>
      <child>rear_right_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>20</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics>
          <damping>0.5</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>
    
    <!-- Odometry Publisher -->
    <plugin filename="gz-sim-odometry-publisher-system"
            name="gz::sim::systems::OdometryPublisher">
      <odom_frame>odom</odom_frame>
      <robot_base_frame>chassis</robot_base_frame>
      <odom_topic>/odom</odom_topic>
      <odom_publish_frequency>20</odom_publish_frequency>
    </plugin>
    
    <!-- Joint Force Plugins - Left Side (front + rear controlled together) -->
    <plugin filename="gz-sim-apply-joint-force-system" 
            name="gz::sim::systems::ApplyJointForce">
      <joint_name>front_left_wheel_joint</joint_name>
    </plugin>
    
    <plugin filename="gz-sim-apply-joint-force-system" 
            name="gz::sim::systems::ApplyJointForce">
      <joint_name>rear_left_wheel_joint</joint_name>
    </plugin>
    
    <!-- Joint Force Plugins - Right Side (front + rear controlled together) -->
    <plugin filename="gz-sim-apply-joint-force-system" 
            name="gz::sim::systems::ApplyJointForce">
      <joint_name>front_right_wheel_joint</joint_name>
    </plugin>
    
    <plugin filename="gz-sim-apply-joint-force-system" 
            name="gz::sim::systems::ApplyJointForce">
      <joint_name>rear_right_wheel_joint</joint_name>
    </plugin>
    
  </model>
</sdf>
